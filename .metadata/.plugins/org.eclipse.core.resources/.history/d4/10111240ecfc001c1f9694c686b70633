package com.java;

public class CloneArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a[]= {5,6,8,4};
		
		int b[]=a.clone();
		
		b[0]++;
		
		System.out.println("Contents of a[]");
		
		for(int i=0;i<a.length;i++) 
		{
			System.out.println(a[i]+" ");
			
		}
	}

}
