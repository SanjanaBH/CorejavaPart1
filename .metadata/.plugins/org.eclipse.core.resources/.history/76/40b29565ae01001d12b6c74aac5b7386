package Main;


class abc{
	int a,b;
	void getdata() {
		a=10;
		b=2;
	}
	
}
class sum extends abc{
	int sum;
	
	void sum(){
		sum=a+b;
		System.out.println("Addition is:"+sum);
	}
}


class sub extends abc{
	int sub;
	
	void sum(){
		sub=a-b;
		System.out.println("Addition is:"+sub);
	}
}

public class MultiLevelExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
