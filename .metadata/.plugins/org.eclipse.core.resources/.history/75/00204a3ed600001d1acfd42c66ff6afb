package Main;



class NewCars{
	String brand;
	
	NewCars(String brand)
	{
		this.brand= brand;
	}
	void brand() {
		System.out.println("Car Brand is:"+brand);
	}
}
public class Car {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		NewCars n1=new NewCars("Ford");
		System.out.println("Car Brand is"+n1);

	}

}
